<% if current_page.last? %>
  <li class="disabled">
    <%= content_tag :a do %>
      <i class="fa fa-chevron-right" aria-hidden="true"></i>
    <% end %>
  </li>
<% else %>
  <li class="waves-effect">
    <%= link_to url, :rel => 'next', :remote => remote do %>
    <i class="fa fa-chevron-right" aria-hidden="true"></i>
    <% end %>
  </li>
<% end %>
